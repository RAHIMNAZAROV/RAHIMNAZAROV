#include <iostream>
using namespace std;

int main() {
    int num;
    cout << "Введіть число: ";
    cin >> num;

    int a = 0, b = 1;
    cout << "Числа Фібоначчі: ";
    while (b < num) {
        cout << b << " ";
        int temp = b;
        b += a;
        a = temp;
    }
    cout << endl;
    return 0;
}
