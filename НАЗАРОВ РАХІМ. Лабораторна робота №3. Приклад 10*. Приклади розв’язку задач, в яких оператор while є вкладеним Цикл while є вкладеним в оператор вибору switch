#include <iostream>

using namespace std;

int main() {
    int a;
    int s = 0;
    int i;
    
    a = 7;  // можна змінити значення для перевірки інших випадків

    // вибір алгоритму рішення задачі в залежності від значення a
    switch (a) {
        case 1:
            i = 0;
            while (i < 100) {
                i = i + 5;
                s = s + i;
            }
            break;
        case 2:
            i = 0;
            while (i < 20) {
                i = i + 5;
                s = s + i;
            }
            break;
        case 3:
            i = 0;
            while (i < 10) {
                i = i + 5;
                s = s + i;
            }
            break;
        default:
            s = 100;
            break;
    }
    
    cout << "s = " << s << endl;

    return 0;
}
